<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sentaxlab.bbazaarshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gb"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gb"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sentaxlab.bbazaarshop.MyOrder"></activity>
        <activity android:name="com.sentaxlab.bbazaarshop.TrackOrder" />
        <activity android:name="com.sentaxlab.bbazaarshop.PlaceOrderActivity" />
