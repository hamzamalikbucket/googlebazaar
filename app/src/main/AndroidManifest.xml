        <activity android:name="com.sentaxlab.bbazaarshop.DetailMarketing" />
        <activity android:name="com.sentaxlab.bbazaarshop.DetailProductActivity" />
        <activity android:name="com.sentaxlab.bbazaarshop.AboutUsActivity" />
        <activity android:name="com.sentaxlab.bbazaarshop.RegisterNowActivity" />
        <activity android:name="com.sentaxlab.bbazaarshop.LoginActivity" />
        <activity android:name="com.sentaxlab.bbazaarshop.HomeActivity" />
        <activity android:name="com.sentaxlab.bbazaarshop.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>